{
  "name": "rundeck-vue-ui",
  "version": "1.0.0",
  "description": "Rundeck User Interface",
  "author": "jesse@rundeck.com",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/vue-cli-service build --watch --mode development",
    "start": "npm run dev",
    "lint": "eslint --ext .js,.vue src",
    "build:ci": "npm ci && npm run build",
    "build": "run-script-os",
    "build:darwin:linux": "./node_modules/.bin/vue-cli-service build --mode production",
    "build:win32": "node_modules/.bin/vue-cli-service.js build --mode production"
  },
  "dependencies": {
    "ant-design-vue": "^1.5.4",
    "axios": "^0.18.0",
    "brace": "^0.11.1",
    "core-js": "^3.6.4",
